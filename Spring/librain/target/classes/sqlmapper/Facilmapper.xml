<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="lib.employee.facility.model.FacilitiesDAO">
  <select id="facSelect" resultType="lib.employee.facility.model.FacilitiesDTO">
    select * from Facilities
  </select>
  
  <insert id="facinsert" parameterType="lib.employee.facility.model.FacilitiesDTO">
   insert into Facilities values(#{Fac_serialnum},#{Fac_name},#{Fac_address })
  </insert>
</mapper>