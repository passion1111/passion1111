<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="EMP2">
  <select id="emp22" resultType="kr.bit.dao.Emp2">
  select * from emp2 where name=#{name}
  
  </select>
  <select id="selectall" resultType="kr.bit.dao.Emp2">
  select empno from emp2
  </select>
  </mapper>