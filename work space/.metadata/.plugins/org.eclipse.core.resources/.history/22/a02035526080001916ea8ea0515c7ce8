package personal;

public class Personal_homawork1 {
			public static void main(String[] args) {
			int i,j = 0,sum = 0,d = 0;
			for(i=1;i<=5;i++) {
				sum+=i;
				d=sum;
				j=d;
				
			}
			System.out.println(sum);
			System.out.println(i);
			System.out.println(d);
			System.out.println(j);
			}

}
