package array3;
//1차원 배열 String[] str={'a','k','P','D','y'}

public class Mainentry2 {
	public static void main(String[] args) {
		 String[] str={"a","k","P","D","y"};
		 char[] str2={'a','k','P','D','y'};
	
			for(int i=0;i<str.length;i++) {
				System.out.println(str[i]);
				
			}
			System.out.println("\n=================");
			for(int i=0;i<str2.length;i++) System.out.println("\t"+str2[i]);
			
		
		}
	}
	