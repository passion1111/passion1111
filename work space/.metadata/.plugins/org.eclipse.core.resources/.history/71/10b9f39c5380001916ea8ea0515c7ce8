package ex05_continue_break;
//break,continue
public class Ex05_multibreak {
		public static void main(String[] args) {
			
			for(int i=1;i<101;i++) {
				if(i%2!=0) continue;
				
				System.out.print(i+"\t");
				if(i%10==0) System.out.println(" ");
			}
		
		}
		}










/*	for(int i=1;i<11;i++) {
if(i==7)break;
System.out.println(i);*/

