package ex02.method;

public class MainEntry {
	
	public static void loopLine() {
		for(int i =1; i < 7 ; i++) {
			System.out.println("--------------");
		}
	}
	
	public static void loopLine(int n) {
		for(int i =1; i <= n ; i++) {
			System.out.println("*************");
		}
	}
	
	public static void main(String[] args) {
		loopLine();
		
		loopLine(3);
	}
}






