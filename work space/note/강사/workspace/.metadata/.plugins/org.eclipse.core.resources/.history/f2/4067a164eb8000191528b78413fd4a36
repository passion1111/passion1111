package ex01.array;

public class MainEntry {
	public static void main(String[] args) {
		//1.
		char[] ch;	//배열선언
		ch = new char[4];  //배열생성, 메모리에 할당
		
		//2.
		char[] ch2 = new char[4]; // 배열선언 및 생성
		//배열 초기화
		ch[0] ='J';
		ch[1] ='A';
		ch[2] ='V';
		ch[3] ='A';
		
		//배열의 내용 출력
		System.out.println("ch[2] = " + ch[2]);
		
		
	}
}








