
package day03.operator;
	
public class Ex01_Opearator4 {
	public static void main(String[] args) {
		   					//논리 연산자:&  &&(and), |  || (or) , ^(xor)
	
	      int x=10,y=20,z=30;
	      boolean flag;
	      
	      flag=(x>y)&&(y>z);
	      System.out.println(flag);
	      
		
				
		
	
	}//end main

}//end class




