package personal;
import java.util.*;
public class Personal3_multiples_sum {
	
	public static void main(String[] args) {
		
		  
			     
			  
		
			   int arr[] = new int [60];
			 
			     int j=0;   //배열순서
			     int temp=0; //3의배수 저장변수
			     Random rand = new Random();
			    
			     for(int i = 1; i < 50; i++)  //자연수 1부터 50미만까지
			    {
			        if(i%3==0|i%5==0) {   //3또는 5의 배수라면
			        	
			        	  
			        	temp=i;
			        	
						arr[rand]=temp;     //0부터 3,5의 배수 저장
						
						
			        					
			        	
			       //counter[number[i]]++
			        	 System.out.println(arr[j]);	  //배열저장확인.
			        	 
			        }
			       
			        arr[rand] = arr[rand] + 1;   //배열순서대로 저장
			      
			   
			    }
			
			    }
			    
				
			    }
			    




//결과값을 다시 배열에 저장할려면 어떻게해야하는지 찾아보기  배수저

 

