package personal;

public class Personal_homawork1 {
			public static void main(String[] args) {
			int i,j = 0,sum = 0,d = 0,c=0,e=0;
			for(i=1;i<=5;i++) {
				if(i==1)sum+=i;
				if(i==2)d=sum+i;
			    if(i==3)j=d+i;
				if(i==4)c=j+i;
				if(i==5)e=c+i;
			}
			System.out.println(sum);
			System.out.println(i);
			System.out.println(d);
			System.out.println(j);
			}

}
