package personal;

public class Personal_homawork1 {
			public static void main(String[] args) {
			int i,j = 0,sum = 0,sum1 = 0,sum2=0,sum3=0,sum4=0;
			for(i=1;i<=5;i++) {
				sum+=i;
				if(sum==3) {
					sum1=sum-i;
					sum2=sum+sum;
					sum3=sum;
				}					
				sum4=sum-i;
			}
			
			System.out.println(sum1);
			System.out.println(sum2);
			
				
				
			
			}
		
			
			}
		



