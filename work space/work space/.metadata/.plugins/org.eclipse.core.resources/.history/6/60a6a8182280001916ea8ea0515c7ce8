package loofcontrol;
//1부터 100까지중에서  짝수만 출력 한행에 10개씩 출력
public class For3 {
	public static void main(String[] args) {
	int i,j;	
		for(i=1;i<=100;i++) {
			if(i%2==0) {
				   
					  System.out.print(i +"\t");
									
				
				
			}
				if(i%20==0)
					System.out.println("\n");
				
			
			
		}
			
			
			
		
	}

}
