package personal;

public class Personal_array_practice {
	public static void main(String[] args) {
		int sum=0,sum1=0,sum2=0,sum3=0,sum4=0,sum5=0;
		float average=0f;
		int[] score=new int[] {1,2,3,4,5};
		for(int i=0;i<score.length;i++) {
			sum+=score[i-1];
			sum2+=score[i-2];
			sum3+=score[i-3];
			sum4+=score[i-4];
			sum5 = score[i];
			
		}
		System.out.println(sum+sum2+sum3+sum4+sum5);
	}

}
