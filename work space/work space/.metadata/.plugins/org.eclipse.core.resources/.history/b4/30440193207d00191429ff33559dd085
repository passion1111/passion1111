
package day03.operator;


	//최단산쉬관논삼대콤
public class Ex01_Opearator3 {
	public static void main(String[] args) {
		   					//shift 연산자:<<,>>  ,<<<
		int x=8,result;
		//result= x<<2;  //left shift: 원래값 * 2^bit수
		result=x>>3;     //right shif: 원래값/2^bit수
		System.out.println("right shif result:"+result);  //1
		//System.out.println("lefit shif result:"+result);  //32
		
	}//end main

}//end class



