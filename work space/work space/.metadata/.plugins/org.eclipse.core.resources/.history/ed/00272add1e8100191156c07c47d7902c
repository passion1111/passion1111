package array2;


// ¹®Á¦3
//      10 *20 °ª
//      1 *  2  2
//      5 *  7  35

//    10  9  13 25
public class Mainentry_quiz3 {
	public static void main(String[] args) {

		int[][] arr={{10,20,0},{1,2,0},{5,7,0}};
		
		for(int i=0;i<arr.length;i++) {
			arr[arr.length-1][i]=arr[i][arr.length-2]*arr[i][arr.length-3];
			
			System.out.println(arr[2][i]);
		}
		
		
		
		
		
		
		
	}
}

