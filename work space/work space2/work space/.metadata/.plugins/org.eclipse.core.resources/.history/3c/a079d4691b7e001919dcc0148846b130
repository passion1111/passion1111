package personal;
//경우의 수 6가지 구하기
//a<b,a<c
//a>b,a>c
//a=b,a=c
//조건:0<a,b,c
import java.util.Scanner;

public class Personal_quiz2 {
   public static void main(String[] args) {
	   Scanner sc=new Scanner(System.in);
	   System.out.println("x,y,z를 차례대로 입력해주세요: ");
	   int x=sc.nextInt();
	   int y=sc.nextInt();
	   int z=sc.nextInt();
	   int max,min,mid;
	   max=(x>y)?(x>z)?(x>z):                          :(x<y);
	   
	   
}
}
