package personal;

public class Personal_homework1_1 {
		public static void main(String[] args) {
			int a,b = 0,c = 0,d = 0,e = 0,f = 0;
			for(a=1;a<=5;a++)
			{
				e+=a;
				f=e-a;
				for(b=1;b<=3;b++)
				{
					c+=b;
					d=c-b;		
				}
				
			}
			System.out.println(c);
			System.out.println((c+d+e+f+b));
		}
}
