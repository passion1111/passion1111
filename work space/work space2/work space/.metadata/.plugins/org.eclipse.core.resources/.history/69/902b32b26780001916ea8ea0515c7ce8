package ex02_for_star;

public class Personal_star9 {
			public static void main(String[] args) {
				int i,j,d;
				for(d=1;d<=7;d++)
				{for(j=1;j<d;j++) {
					System.out.print(" ");
				}
				for(i=1;i<=5-2*(d-1);i++) System.out.print("*");
						System.out.println("");
}
				}
				
				
			}


