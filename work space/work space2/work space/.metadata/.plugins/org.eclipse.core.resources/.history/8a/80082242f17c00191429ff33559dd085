package personal;
import java.util.*;
public class Personal3_multiples_sum {
	
	public static void main(String[] args) {
		
		  
			     
			  
			     
			     int[] arr = new int[60];
			 
for(int j1=0;j1<50;j1++) {
			     for(int i = 1; i < 50; i++)
			    {
			        if((i%3)==0) {
			        		for(int j=j1;j<arr.length;j++) {
			        			int temp=i;
			    	   arr[i]=temp;
			    	   System.out.println(temp);
			        }
			     
			    }
			    }
			    
			    }
			    
}
}


//결과값을 다시 배열에 저장할려면 어떻게해야하는지 찾아보기  배수저

 

