package ex05_continue_break;
//break,continue
public class Ex05_multibreak {
		public static void main(String[] args) {
			
				for(int i=1;i<11;i++) {
			if(i==7)continue;//조건문,증감식으로 가게하는 역할 continue
			System.out.println(i);
			}
		}










