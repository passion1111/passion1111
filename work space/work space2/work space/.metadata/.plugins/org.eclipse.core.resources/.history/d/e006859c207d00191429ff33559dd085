
package day03.operator;


	//최단산쉬관논삼대콤
public class Ex01_Opearator3 {
	public static void main(String[] args) {
		   					//관계연산자:>,<,>=,<=,==같다,!=같지않다
				int x=10,y=20;
				
				if(x>y)
					System.out.println("max:"+x);
				else
					System.out.println("max: "+ x);
	
	}//end main

}//end class



