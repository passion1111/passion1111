
package day03.operator;
	//java.lang pakage는 자동으로 import해준다.

	//최단산쉬관논삼대콤
public class Ex01_Opearator4 {
	public static void main(String[] args) {
		   					//관계연산자:>,<,>=,<=,==같다,!=같지않다
				int x=10,y=20;
			
				if(x!=y)  //x가 y와같지않으면
					System.out.println("같지 않습니다");
					else
						System.out.println("같습니다");
					
		/*
		 * if(x>y) System.out.println("max:"+x); else System.out.println("max: "+ y); max값 찾기
		 */
	
	}//end main

}//end class



