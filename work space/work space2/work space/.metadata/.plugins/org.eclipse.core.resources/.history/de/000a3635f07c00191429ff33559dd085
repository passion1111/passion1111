package personal;
import java.util.*;
public class Personal3_multiples_sum {
	
	public static void main(String[] args) {
		
		  
			     
			  
			     
			     int[] arr = new int[60];
			 

			     for(int i = 1; i < 50; i++)
			    {
			        if((i%3)==0) {
			    	   arr[i]=i;
			    	   System.out.println(arr);
			        }
			     
			    }
			    
			    }
			    
}


//결과값을 다시 배열에 저장할려면 어떻게해야하는지 찾아보기  배수저

 

