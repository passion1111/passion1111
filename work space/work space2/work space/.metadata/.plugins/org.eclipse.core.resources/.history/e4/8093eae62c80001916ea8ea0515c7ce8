package loofcontrol;
//구구단 전체 출력
//2*1=2 3*1=3 4*1=4
public class For6 {
			public static void main(String[] args) {
				int i,j,d,e;
				 i=2;
				
				
				  for(d=1;d<=9;d++) {
				    	for(i=1;i<=9;i++) {
				    		e=i*j;
				    		System.out.println( i j  e);
				    	}
					    i++;  
				  }
					  
				    	   
				
				
				
			}

}